@model SetResult
@section head{
    <link href="//vjs.zencdn.net/5.10.8/video-js.min.css" rel="stylesheet">
    <script src="//vjs.zencdn.net/5.10.8/video.min.js"></script>
    <link rel="stylesheet" href="~/css/ListItemStyle.css" />
    <link href="~/css/videojs-resolution-switcher.css" rel="stylesheet" />
    <script src="~/js/videojs-resolution-switcher.js"></script>
    @if (ViewBag.IsMobile)
    {
        <link rel="stylesheet" href="~/css/VideoStyle.mobile.css" />
    }
    else
    {
        <link rel="stylesheet" href="~/css/VideoStyle.css" />
    }
}


@section header{
    <div class="mdl-layout-spacer"></div>
    <a id="app" href="https://github.com/Tlaster/OneEchan" class="mdl-button mdl-js-button mdl-button--icon">
        <i class="material-icons">get_app</i>
    </a>
    <div class="mdl-tooltip" for="app">
        Download App For Your Phone
    </div>

}

<video id="VideoElement" class="mdl-shadow--2dp video-js vjs-default-skin" preload="auto" controls data-setup='{}'>
    @if (Model.OriginalQuality != null)
    {
        <source src="@Model.OriginalQuality" type='video/mp4' label='source' res='720' />
    }
    @if (Model.HighQuality != null)
    {
        <source src="@Model.HighQuality" type='video/mp4' label='720' res='720' />
    }
    @if (Model.MediumQuality != null)
    {
        <source src="@Model.MediumQuality" type='video/mp4' label='480' res='480' />
    }
    @if (Model.LowQuality != null)
    {
        <source src="@Model.LowQuality" type='video/mp4' label='240' res='240' />
    }
    <p class="vjs-no-js">
        To view this video please enable JavaScript, and consider upgrading to a web browser
        that <a href="http://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a>
    </p>
</video>
<script>
    videojs('VideoElement').videoJsResolutionSwitcher()
</script>


